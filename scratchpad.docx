def calcuate_average_grade(grades):
    sum=0
    for key in grades:
        sum+=grades.get(key)

    if len(grades)==0:
        return 0.0
    else:
        return sum/len(grades)
    

marks={
    'Math':90,
    'Science':88,
    'History':92,
    'English':95
}

marksnull={}

print(calcuate_average_grade(marks)) #91.25
print(calcuate_average_grade(marksnull)) #0.0


